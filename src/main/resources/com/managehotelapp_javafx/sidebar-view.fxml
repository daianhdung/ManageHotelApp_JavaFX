                    <Font name="System Bold" size="29.0" />
                </font>
            </Button>
            <Button fx:id="btn_invoice" alignment="BASELINE_LEFT" layoutY="312.0" mnemonicParsing="false" prefHeight="55.0" prefWidth="242.0" style="-fx-background-color: #00CCFF;" text="INVOICE">
                <font>
                    <Font name="System Bold" size="29.0" />
                </font>
            </Button>
            <Button fx:id="btn_revenue" alignment="BASELINE_LEFT" layoutY="387.0" mnemonicParsing="false" prefHeight="55.0" prefWidth="242.0" style="-fx-background-color: #00CCFF;" text="REVENUE">
                <font>
                    <Font name="System Bold" size="29.0" />
                </font>
            </Button>
        </children>
    </Pane>
</AnchorPane>
