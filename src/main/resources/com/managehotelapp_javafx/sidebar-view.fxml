            fx:controller="com.managehotelapp_javafx.controller.SidebarController"
            prefHeight="400.0" prefWidth="600.0">


    <Pane layoutX="14.0" layoutY="44.0" prefHeight="445.0" prefWidth="242.0">
        <children>
            <Button fx:id="btn_home" alignment="BASELINE_LEFT" layoutY="52.0" mnemonicParsing="false" prefHeight="55.0" prefWidth="242.0" style="-fx-background-color: #00CCFF;" text="HOME">
                <font>
                    <Font name="System Bold" size="29.0" />
                </font>
            </Button>
            <Button fx:id="btn_room" alignment="BASELINE_LEFT" layoutY="115.0" mnemonicParsing="false" prefHeight="55.0" prefWidth="242.0" style="-fx-background-color: #00CCFF;" text="ROOM">
                <font>
                    <Font name="System Bold" size="29.0" />
                </font>
            </Button>
            <Button fx:id="btnBooking" alignment="BASELINE_LEFT" layoutY="177.0" mnemonicParsing="false" onAction="#onBooking" prefHeight="55.0" prefWidth="242.0" style="-fx-background-color: #00CCFF;" text="BOOKING">
